digraph {
	CONNECTED [label=CONNECTED]
	IDLE [label=IDLE]
	SEND_VERSION_RESPONSE [label=SEND_VERSION_RESPONSE]
	DISCONNECTED [label=DISCONNECTED]
		IDLE -> SEND_VERSION_RESPONSE [label=VersionRequest]
		SEND_VERSION_RESPONSE -> CONNECTED [label=VersionResponse]
		SEND_VERSION_RESPONSE -> SEND_VERSION_RESPONSE [label="ErrorResponse / HostNameRequest / ClientNameRequest"]
		IDLE -> IDLE [label=VersionResponse]
		CONNECTED -> CONNECTED [label="EventsOnRequest / EventsOffRequest / EchoResponse / ErrorResponse / HostNameRequest / EchoRequest / ClientNameRequest"]
}
