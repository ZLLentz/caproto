	CLOSED [label=CLOSED]
	SEND_VERSION_REQUEST [label=SEND_VERSION_REQUEST]
	AWAIT_VERSION_RESPONSE [label=AWAIT_VERSION_RESPONSE]
	ERROR [label=ERROR]
	CONNECTED [label=CONNECTED]
		SEND_VERSION_REQUEST -> AWAIT_VERSION_RESPONSE [label=VersionRequest]
		AWAIT_VERSION_RESPONSE -> ERROR [label=ErrorResponse]
		AWAIT_VERSION_RESPONSE -> CONNECTED [label=VersionResponse]
		CONNECTED -> ERROR [label=ErrorResponse]
		AWAIT_VERSION_RESPONSE -> AWAIT_VERSION_RESPONSE [label="EchoRequest / HostNameRequest / EchoResponse / ClientNameRequest"]
		SEND_VERSION_REQUEST -> SEND_VERSION_REQUEST [label=VersionResponse]
		CONNECTED -> CONNECTED [label="EchoRequest / HostNameRequest / EchoResponse / EventsOffRequest / EventsOnRequest / ClientNameRequest"]
}