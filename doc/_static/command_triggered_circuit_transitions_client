digraph {
	CLOSED [label=CLOSED]
	SEND_VERSION_REQUEST [label=SEND_VERSION_REQUEST]
	ERROR [label=ERROR]
	AWAIT_VERSION_RESPONSE [label=AWAIT_VERSION_RESPONSE]
	CONNECTED [label=CONNECTED]
		SEND_VERSION_REQUEST -> ERROR [label=ErrorResponse]
		SEND_VERSION_REQUEST -> AWAIT_VERSION_RESPONSE [label=VersionRequest]
		SEND_VERSION_REQUEST -> CONNECTED [label=VersionResponse]
		AWAIT_VERSION_RESPONSE -> ERROR [label=ErrorResponse]
		AWAIT_VERSION_RESPONSE -> CONNECTED [label=VersionResponse]
		CONNECTED -> ERROR [label=ErrorResponse]
		CONNECTED -> AWAIT_VERSION_RESPONSE [label=VersionRequest]
		AWAIT_VERSION_RESPONSE -> AWAIT_VERSION_RESPONSE [label="EchoRequest / ClientNameRequest / EchoResponse / VersionRequest / HostNameRequest"]
		SEND_VERSION_REQUEST -> SEND_VERSION_REQUEST [label="EchoResponse / EchoRequest"]
		CONNECTED -> CONNECTED [label="EchoRequest / VersionResponse / ClientNameRequest / AccessRightsResponse / EchoResponse / HostNameRequest"]
}